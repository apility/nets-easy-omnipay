{
    "name": "apility/nets-easy-omnipay",
    "type": "library",
    "description": "Nets Easy driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "nets",
        "nets-easy",
        "nexi"
    ],
    "homepage": "https://github.com/apility/nets-easy-omnipay",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        }
    ],
    "require": {
        "php": ">=7.1",
        "omnipay/common": "^3.0",
        "symfony/http-foundation": "^4.0|^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^2.0",
        "symfony/http-client": "*",
        "php-http/httplug": "^2.4",
        "nyholm/psr7": "^1.8",
        "larapack/dd": "^1.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Apility\\Omnipay\\NetsEasy\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}